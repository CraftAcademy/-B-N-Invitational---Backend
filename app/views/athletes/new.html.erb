<div class="form">
<%= form_with model: Athlete.new, local: true do |form| %>
  <p>
  <%= form.label :name %>
  <br>
  <%= form.text_field :name %>
  </p>
  <p>
  <%= form.label :home %>
  <br>
  <%= form.text_field :home %>
  </p>
  <p>
  <%= form.label :age %>
  <br>
  <%= form.text_field :age %>
  </p>
  <p>
  <%= form.label :starttime %>
  <br>
  <%= form.text_field :starttime%>
  </p>
  <p><%= form.file_field :image %></p>
  <%= form.submit %>
<% end %>
</div>
