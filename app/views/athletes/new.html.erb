<%= form_with model: Athlete.new, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :home %>
  <%= form.text_field :home %>
  <%= form.label :age %>
  <%= form.text_field :age %>
  <%= form.label :starttime %>
  <%= form.text_field :starttime %>
  <%= form.file_field :image %>
  <%= form.submit %>
<% end %>
